{
    "name": "gos/pubsub-router-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PubSub Router Bundle",
    "keywords": ["PubSub Bundle", "PubSub", "Bundle", "Redis", "WAMP", "ZMQ"],
    "homepage": "https://github.com/GeniusesOfSymfony/PubSubRouterBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Johann Saunier",
            "email": "johann_27@hotmail.fr"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/config": "^5.2",
        "symfony/console": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/polyfill-php80": "^1.22",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.78",
        "phpstan/phpstan-phpunit": "^0.12.17",
        "phpstan/phpstan-symfony": "^0.12.20",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "Gos\\Bundle\\PubSubRouterBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Gos\\Bundle\\PubSubRouterBundle\\Tests\\": "tests/" }
    },
    "suggest": {
        "ext-dom": "* to use the XML file loader"
    },
    "minimum-stability": "dev"
}
