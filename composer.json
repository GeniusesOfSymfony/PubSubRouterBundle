{
    "name": "gos/pubsub-router-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PubSub Router Bundle",
    "keywords": ["PubSub Bundle", "PubSub", "Bundle", "Redis", "WAMP", "ZMQ"],
    "homepage": "https://github.com/GeniusesOfSymfony/PubSubRouterBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Johann Saunier",
            "email": "johann_27@hotmail.fr"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/config": "^4.4 || ^5.2",
        "symfony/console": "^4.4 || ^5.2",
        "symfony/dependency-injection": "^4.4 || ^5.2",
        "symfony/deprecation-contracts": "^2.1",
        "symfony/http-foundation": "^4.4 || ^5.2",
        "symfony/http-kernel": "^4.4 || ^5.2",
        "symfony/polyfill-php80": "^1.22",
        "symfony/yaml": "^4.4 || ^5.2"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.1.2",
        "phpstan/extension-installer": "^1.0.5",
        "phpstan/phpstan": "^0.12.50",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/phpstan-symfony": "^0.12.8",
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": { "Gos\\Bundle\\PubSubRouterBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Gos\\Bundle\\PubSubRouterBundle\\Tests\\": "tests/" }
    },
    "minimum-stability": "dev"
}
