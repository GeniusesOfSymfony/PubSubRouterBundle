{
    "pattern": "chat\/{user}",
    "patternRegex": "#^chat(?:\/(?P<user>\\d+))?$#sD",
    "callback": "strlen()",
    "requirements": {
        "user": "\\d+"
    },
    "class": "Gos\\Bundle\\PubSubRouterBundle\\Router\\Route",
    "defaults": {
        "user": 42
    },
    "options": {
        "compiler_class": "Gos\\Bundle\\PubSubRouterBundle\\Router\\RouteCompiler",
        "foo": "bar"
    }
}
